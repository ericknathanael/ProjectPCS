<Window x:Class="ProjectPCS.pembayaranWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectPCS"
        mc:Ignorable="d"
        Title="pembayaranWindow" Height="120" Width="300">
    <Grid>
        <Button x:Name="btSubmit" Content="Button" HorizontalAlignment="Left" Margin="101,53,0,0" VerticalAlignment="Top" Width="98" Height="28" Click="btSubmit_Click"/>
        <Label Content="Pembayaran :" HorizontalAlignment="Left" Margin="15,16,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbBayaran" HorizontalAlignment="Left" Height="23" Margin="101,19,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183"/>
    </Grid>
</Window>
