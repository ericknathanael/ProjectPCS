<Window x:Class="ProjectPCS.menuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectPCS"
        mc:Ignorable="d"
        Title="menuWindow" Height="450" Width="800">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem x:Name="menuTrans" Header="Transaksi" Click="menuTrans_Click" />
            <MenuItem x:Name="menuManager" Header="Manager" Click="menuKaryawan_Click" />
            <MenuItem x:Name="menuLaporan" Header="Laporan" Click="menuLaporan_Click" />
            <MenuItem x:Name="masterMenu" Header="Master Menu" Click="masterMenu_Click" />
        </Menu>
        <StackPanel>
            
        </StackPanel>
    </DockPanel>
</Window>
